{
    "env": {
      "es6": true
    },
    "extends": [
      "standard",
      "plugin:prettier/recommended",
      "plugin:ava/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "valid-jsdoc": "error",
      "no-console": "off",
      "linebreak-style": [
        "error",
        "unix"
      ],
      "ava/no-async-fn-without-await": "warn"
    }
 }